{
    "name": "tpwdag/typo3-text-flow",
    "type": "typo3-cms-extension",
    "description": "Optimizes text flow with dynamic hyphenation",
    "license": "MIT",
    "authors": [
      {
                  "name": "TPWD AG",
        "email": "cab@tpwd.de",
        "role": "Developer"
      }
    ],
    "require": {
      "typo3/cms-core": "^12.0",
      "typo3/cms-fluid": "^12.0"
    },
    "extra": {
      "typo3/cms": {
        "extension-key": "text_flow"
      }
    },
    "autoload": {
      "psr-4": {
        "Tpwdag\\TextFlow\\": "Classes/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tpwdag\\TextFlow\\Tests\\": "Tests/"
      }
    },
    "require-dev": {
      "typo3/testing-framework": "^8.0",
      "phpstan/phpstan": "^1.10",
      "typo3/coding-standards": "^0.7"
    }
  }