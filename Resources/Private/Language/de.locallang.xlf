        <body>
            <!-- Plugin Labels -->
            <trans-unit id="plugin.textflow.title">
                <source>TextFlow</source>
                <target>TextFlow</target>
            </trans-unit>
            <trans-unit id="plugin.textflow.description">
                <source>Optimize text flow with intelligent hyphenation</source>
                <target>Optimieren Sie den Textfluss mit intelligenter Silbentrennung</target>
            </trans-unit>
            <trans-unit id="plugin.textoptimizer.title">
                <source>Text Optimizer</source>
                <target>Text-Optimierer</target>
            </trans-unit>
            <trans-unit id="plugin.textoptimizer.description">
                <source>Tool for text optimization and hyphenation</source>
                <target>Werkzeug zur Textoptimierung und Silbentrennung</target>
            </trans-unit>

            <!-- Form Labels -->
            <trans-unit id="form.text.label">
                <source>Text to optimize:</source>
                <target>Zu optimierender Text:</target>
            </trans-unit>
            <trans-unit id="form.text.placeholder">
                <source>Enter your text here...</source>
                <target>Geben Sie hier Ihren Text ein...</target>
            </trans-unit>
            <trans-unit id="form.submit.label">
                <source>Optimize Text</source>
                <target>Text optimieren</target>
            </trans-unit>
            <trans-unit id="form.help.text">
                <source>Please enter the text you want to optimize.</source>
                <target>Bitte geben Sie den zu optimierenden Text ein.</target>
            </trans-unit>

            <!-- Result Labels -->
            <trans-unit id="result.original.title">
                <source>Original Text</source>
                <target>Original-Text</target>
            </trans-unit>
            <trans-unit id="result.optimized.title">
                <source>Optimized Text</source>
                <target>Optimierter Text</target>
            </trans-unit>
            <trans-unit id="result.empty.text">
                <source>No text available for optimization</source>
                <target>Kein Text zur Optimierung verfügbar</target>
            </trans-unit>

            <!-- Error Messages -->
            <trans-unit id="error.no_text">
                <source>Please enter a text for optimization</source>
                <target>Bitte geben Sie einen Text zur Optimierung ein</target>
            </trans-unit>
            <trans-unit id="error.processing">
                <source>Error while processing the text</source>
                <target>Fehler bei der Textverarbeitung</target>
            </trans-unit>

            <!-- FlexForm Labels -->
            <trans-unit id="flexform.settings">
                <source>Settings</source>
                <target>Einstellungen</target>
